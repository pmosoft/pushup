<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="$packNm$.$PgmNm$Dao">

<select id="select$PgmNm$List" parameterType="java.util.HashMap" resultType="java.util.HashMap">

	-- $PgmNm$Dao.select$PgmNm$List
    SELECT '제목1' AS TITLE ,'내용1' AS CONTENT ,'' AS REG_DTM ,'ADMIN' AS REG_USR_ID ,'' AS UPD_DTM ,'' AS UPD_USR_ID UNION ALL            
    SELECT '제목2' AS TITLE ,'내용2' AS CONTENT ,'' AS REG_DTM ,'ADMIN' AS REG_USR_ID ,'' AS UPD_DTM ,'' AS UPD_USR_ID UNION ALL
    SELECT '제목3' AS TITLE ,'내용3' AS CONTENT ,'' AS REG_DTM ,'ADMIN' AS REG_USR_ID ,'' AS UPD_DTM ,'' AS UPD_USR_ID UNION ALL
    SELECT '제목4' AS TITLE ,'내용4' AS CONTENT ,'' AS REG_DTM ,'ADMIN' AS REG_USR_ID ,'' AS UPD_DTM ,'' AS UPD_USR_ID    
			
</select>

<select id="select$PgmNm$Cnt" parameterType="java.util.HashMap" resultType="int">
	-- $PgmNm$Dao.select$PgmNm$Cnt
	SELECT COUNT(*) CNT
	FROM   DUAL
</select>

<insert id="insert$PgmNm$" parameterType="java.util.HashMap">

	-- $PgmNm$Dao.insert$PgmNm$
	INSERT INTO TDACM00010
	(
	     PKG_FUL_NM
		,PKG2_NM
		,REG_DTM
		,REG_USR_ID
		,UPD_DTM
		,UPD_USR_ID
	) VALUES (
         #{PKG_FUL_NM}
		,#{PKG2_NM}
		,curdate()
		,#{REG_USR_ID}
		,curdate()
		,#{UPD_USR_ID}
	)

</insert>

<delete id="delete$PgmNm$" parameterType="java.util.HashMap">
	-- $PgmNm$Dao.delete$PgmNm$
	DELETE FROM TDACM00040 WHERE COL_STS_CD = #{COL_STS_CD}
</delete>

<update id="update$PgmNm$" parameterType="java.util.HashMap">

	-- $PgmNm$Dao.update$PgmNm$
	UPDATE TDACM00010
	SET  PKG2_NM      = #{PKG2_NM}
		,PKG3_NM      = #{PKG3_NM}
		,PKG4_NM      = #{PKG4_NM}
		,PKG_HNM      = #{PKG_HNM}
		,PKG_DESC     = #{PKG_DESC}
		,USE_YN       = CASE WHEN #{USE_YN} = 'true' THEN 'Y' ELSE 'N' END
		,UPD_DTM      = curdate()
		,UPD_USR_ID  = #{UPD_USR_ID}
	WHERE  PKG_FUL_NM = #{PKG_FUL_NM}

</update>

</mapper>
